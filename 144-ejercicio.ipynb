   "source": [
    "def factorial(n):\n",
    "    res = 1\n",
    "    for i in range(1,n+1):\n",
    "        res *= i\n",
    "    return res\n",
    "\n",
    "print(factorial(4)) # 1 * 2 * 3 * 4 = 24\n",
    "print(factorial(5)) # 1 * 2 * 3 * 4 * 5 = 120"
   ]
  }
