   ],
   "source": [
    "res = []\n",
    "for i in range(5):\n",
    "    if(i % 2 == 0):\n",
    "        res.append(True)\n",
    "    else:\n",
    "        res.append(False)\n",
    "\n",
    "print(res)\n",
    "\n"
   ]
